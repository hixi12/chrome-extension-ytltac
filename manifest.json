{
  "manifest_version": 3,
  "name": "YouTube: Latest Tab Auto Clicker",
  "version": "1.0.0",

  "description": "YouTubeのチャンネルページの動画・ショート・ライブを表示した際に新しい順を自動でクリックします。",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "runAt": "document_start",
      "js": ["content-script.js"]
    }
  ]
}
